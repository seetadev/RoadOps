{
  "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "strict": true,
      "preserveConstEnums": true,
      "noEmit": true,
      "sourceMap": false,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
  },
  "exclude": ["node_modules"]
}
